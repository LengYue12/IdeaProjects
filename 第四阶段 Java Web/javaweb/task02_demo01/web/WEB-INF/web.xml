<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <com.lagou.demo.servlet>
        <com.lagou.demo.servlet-name>RegisterServlet</com.lagou.demo.servlet-name>
        <com.lagou.demo.servlet-class>com.lagou.demo01.com.lagou.demo.servlet.RegisterServlet</com.lagou.demo.servlet-class>
    </com.lagou.demo.servlet>
    <com.lagou.demo.servlet>
        <com.lagou.demo.servlet-name>LoginServlet</com.lagou.demo.servlet-name>
        <com.lagou.demo.servlet-class>com.lagou.demo01.com.lagou.demo.servlet.LoginServlet</com.lagou.demo.servlet-class>
    </com.lagou.demo.servlet>
    <com.lagou.demo.servlet-mapping>
        <com.lagou.demo.servlet-name>LoginServlet</com.lagou.demo.servlet-name>
        <url-pattern>/login</url-pattern>
    </com.lagou.demo.servlet-mapping>
    <com.lagou.demo.servlet-mapping>
        <com.lagou.demo.servlet-name>RegisterServlet</com.lagou.demo.servlet-name>
        <url-pattern>/register</url-pattern>
    </com.lagou.demo.servlet-mapping>
</web-app>